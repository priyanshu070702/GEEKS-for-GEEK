 long long int ans =0;

        long long int sum = 0;

        unordered_map<int,int> mm;

        for(int i=0;i<n;i++){

            sum+=arr[i];

            if(sum==0){

                ans++;

            }

            if(mm[sum]){

                ans+=mm[sum];

            }

            mm[sum]++;

        }

        return ans;
